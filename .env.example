# AWS Configuration
AWS_PROFILE=your-aws-profile
AWS_REGION=us-west-2

# RDS Postgres Configuration
DB_HOST=your-rds-endpoint.rds.amazonaws.com
DB_PORT=5432
DB_NAME=oriole
DB_USER=oriole_user
DB_PASSWORD=your-db-password

# PostgreSQL client environment variables (for psql commands)
PGHOST=your-rds-endpoint.rds.amazonaws.com
PGPORT=5432
PGDATABASE=oriole
PGUSER=oriole_user
PGPASSWORD=your-db-password

# Step Functions
STEP_FUNCTIONS_ARN=arn:aws:states:REGION:ACCOUNT_ID:stateMachine:oriole-experiment-runner

# Agent IDs (get from CDK outputs: npm run deploy)
CLAUDE_HAIKU_AGENT_ID=your-agent-id
CLAUDE_HAIKU_ALIAS_ID=your-alias-id
NOVA_LITE_AGENT_ID=your-agent-id
NOVA_MICRO_AGENT_ID=your-agent-id
NOVA_PRO_AGENT_ID=your-agent-id
NOVA_PREMIER_AGENT_ID=your-agent-id
